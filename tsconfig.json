{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2020",
		"lib": ["es2020"],
		"outDir": "out",
		"sourceMap": true,
		"strict": true,
		"rootDir": "src",
		"paths": { 
			"@api/*": ["./src/api/*"],
 		    "@provider/*": ["./src/provider/*"],
			"@service/*": ["./src/service/*"],
			"@/*": ["./src/*"]
		}
	},
	"exclude": ["node_modules", ".vscode-test"]
}