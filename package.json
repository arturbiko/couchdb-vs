{
	"name": "couchdb-vs",
	"displayName": "CouchDB VS",
	"version": "0.0.1",
	"description": "Datamanagement for CouchDB.",
	"author": {
		"name": "Artur Biko",
		"email": "your.email@example.com"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"main": "./out/extension.js",
	"activationEvents": [],
	"contributes": {
		"configuration": {
			"title": "CouchDB VS",
			"properties": {
				"couchDBVS.host": {
					"type": "string",
					"description": "The URL of the host",
					"default": "",
					"scope": "workspace"
				},
				"couchDBVS.username": {
					"type": "string",
					"description": "The username to use",
					"default": "",
					"scope": "workspace"
				},
				"couchDBVS.password": {
					"type": "string",
					"description": "The password to use",
					"default": "",
					"scope": "workspace"
				}
			}
		},
		"viewsContainers": {
			"activitybar": [
				{
					"icon": "./assets/icon.svg",
					"id": "couchDataView",
					"title": "CouchDB"
				}
			]
		},
		"views": {
			"couchDataView": [
				{
					"id": "couchDBVS.couchDataView",
					"name": "Data"
				}
			]
		},
		"commands": [
			{
				"command": "helloworld.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"dependencies": {
		"nano": "10.1.2"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^5.0.2"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"license": "MIT"
}
